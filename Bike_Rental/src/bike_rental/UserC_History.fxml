<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="497.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/20.0.1" fx:controller="bike_rental.UserC_HistoryController">
